<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jp.targetdigital" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Tarjeta Digital</name>
  <description>
    A basic Framework7 template for PhoneGap.
  </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">
    Apache Cordova Team
  </author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <preference name="android-minSdkVersion" value="14"/>
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
    <preference name="BackupWebStorage" value="none"/>
  </platform>
  <preference name="DisallowOverscroll" value="true"/>
  <universal-links>
    <host name="https://pulos.page.link" scheme="https"/>
    <host name="https://pulos-1526486748191.firebaseapp.com" scheme="https">
      <path url="/__/auth/callback"/>
    </host>
  </universal-links>
  <preference name="AndroidLaunchMode" value="singleTask" />
  <plugin name="cordova-plugin-whitelist" spec="~1.2.0"/>
  <plugin name="cordova-plugin-console" spec="~1.0.1"/>
  <plugin name="cordova-plugin-statusbar" spec="~1.0.1"/>
  <plugin name="cordova-plugin-buildinfo" spec="~2.0.1"/>
  <plugin name="cordova-universal-links-plugin" spec="~1.2.1"/>
  <plugin name="cordova-plugin-browsertab" spec="~0.2.0"/>
  <plugin name="cordova-plugin-inappbrowser" spec="~3.0.0"/>
</widget>
